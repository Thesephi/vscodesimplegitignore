{
    "name": "simplegitignore",
    "displayName": "simplegitignore",
    "description": "Generating your boilerplate .gitignore the dead simple way",
    "version": "0.1.2",
    "publisher": "thesephi",
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#ffcf00"
    },
    "preview": false,
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages", "Other"
    ],
    "activationEvents": [
        "onCommand:extension.simplegitignore"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.simplegitignore",
                "title": "simplegitignore"
            }
        ],
        "menus": {
            "commandPalette": [{
                "command": "extension.simplegitignore",
                "when": "workspaceFolderCount"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Thesephi/vscodesimplegitignore.git"
    },
    "keywords": [
        "multi-root ready"
    ]
}
